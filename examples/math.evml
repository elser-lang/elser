(ns "math")

(storage
 {:external
  [(def mid :uint256)
   (def xer :uint256 {:slot 5})
   (def mid :uint256 => :bool)]
  
  :internal
  [(def start :uint256)
   (def end :uint256 {:slot 15})]
  })

(functions
 {:external
  [
   (defn add [(x :uint256) (y :uint256)] (:uint256)
     (+ x y))
   (defn sub [(x :uint256) (y :uint256)] (:uint256)
     (- x y))
   (defn nothing [] ()
     "nothing")
   ]
  
  :internal
  [
   (defn _pow [(x :uint256) (p :uint256)] (:uint256)
     (** x p))
   ]})
